{
"metadata": {"spec-version": "1.5"},

"display-name": "Isotonic Regressor",
 "configuration-groups": [
    {
      "label": "Basic Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Model Name",
          "name": "modelName"
        },
        {
          "widget-type": "textbox",
          "label": "Features Field",
          "name": "featuresCol"
        },
        {
          "widget-type": "textbox",
          "label": "Label Field",
          "name": "labelCol"
        },
        {
          "widget-type": "textbox",
          "label": "Data Split",
          "name": "datasSplit",
          "widget-attributes": {
            "default": "70:30"
          }
        }
      ]
    },
    {
      "label": "Model Configuration",
      "properties": [
        {
          "widget-type": "select",
          "label": "Isotonic Indicator",
          "name": "isotonic",
          "widget-attributes": {
            "values": [
              "antitonic",
              "isotonic"
            ],
            "default": "isotonic"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Feature Index",
          "name": "featureIndex",
          "widget-attributes": {
            "default": "0"
          }
        }
       ]
    }
  ]
}