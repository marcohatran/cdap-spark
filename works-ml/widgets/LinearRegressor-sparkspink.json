{
"metadata": {"spec-version": "1.5"},

"display-name": "RF Regressor",
 "configuration-groups": [
    {
      "label": "Basic Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Model Name",
          "name": "modelName"
        },
        {
          "widget-type": "textbox",
          "label": "Features Field",
          "name": "featuresCol"
        },
        {
          "widget-type": "textbox",
          "label": "Label Field",
          "name": "labelCol"
        },
        {
          "widget-type": "textbox",
          "label": "Data Split",
          "name": "datasSplit",
          "widget-attributes": {
            "default": "70:30"
          }
        }
      ]
    },
    {
      "label": "Model Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Maximum Iterations",
          "name": "maxIter",
          "widget-attributes": {
            "default": "20"
          }
        },
        {
          "widget-type": "textbox",
          "label": "ElasticNet Mixing",
          "name": "elasticNetParam",
          "widget-attributes": {
            "default": "0.0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Regularization Parameter",
          "name": "regParam",
          "widget-attributes": {
            "default": "0.0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Conversion Tolerance",
          "name": "tol",
          "widget-attributes": {
            "default": "1E-6"
          }
        },
        {
          "widget-type": "select",
          "label": "Solver Algorithm",
          "name": "solver",
          "widget-attributes": {
            "values": [
              "auto",
              "l-bfgs",
              "normal"
            ],
            "default": "auto"
          }
        }
      ]
    }
  ]
}
