{
"metadata": {"spec-version": "1.5"},

"display-name": "GBT Predictor",
 "configuration-groups": [
    {
      "label": "Basic Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Model Name",
          "name": "modelName"
        },
        {
          "widget-type": "select",
          "label": "Model Stage",
          "name": "modelStage",
          "widget-attributes": {
            "values": [
              "experiment",
              "stagging",
              "production",
              "archived"
            ],
            "default": "experiment"
          }
        },
        {
          "widget-type": "select",
          "label": "Model Variant",
          "name": "modelOption",
          "widget-attributes": {
            "values": [
              "best",
              "latest"
            ],
            "default": "latest"
          }
        },
        {
          "widget-type": "select",
          "label": "Model Type",
          "name": "modelType",
          "widget-attributes": {
            "values": [
              "classifier",
              "regressor"
            ],
            "default": "classifier"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Features Field",
          "name": "featuresCol"
        },
        {
          "widget-type": "textbox",
          "label": "Prediction Field",
          "name": "predictionCol"
        }
      ]
    }
  ]
}